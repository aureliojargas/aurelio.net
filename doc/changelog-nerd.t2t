%!includeconf:		../include/kiwi/config.t2t
%!postproc: SITEROOT 	..
%!postproc: (<BODY)	\1 class="doc changelog-nerd"

%!postproc: '(</HEAD>)'	'<script type="text/javascript" src="changelog-nerd.js"></script>\n\1'

% Remove sidebar: Javascripts and inclusion from kiwi/footer.t2t
%!postproc: '<script type="text/javascript" src=".*/kiwi/.*'	''
%!preproc: '^%!include: /a/www/include/kiwi/sidebar.t2t'	''

% não quebra na coluna da data
%!postproc: '(<TD)(>200\d-)'	'\1 nowrap="nowrap"\2'

% uma cor para cada programa/doc
%!postproc: '(<TD)(>txt2tags)'  	'\1 bgcolor="orange"\2'
%!postproc: '(<TD)(>txt2regex)' 	'\1 bgcolor="cyan"\2'
%!postproc: '(<TD)(>sedsed)'    	'\1 bgcolor="silver"\2'
%!postproc: '(<TD)(>Funções ZZ)'	'\1 bgcolor="yellow"\2'
%!postproc: '(<TD)(>sed-HOWTO)' 	'\1 bgcolor="magenta"\2'
%!postproc: '(<TD)(>Coluna)'    	'\1 bgcolor="yellow"\2'
%!postproc: '(<TD)(>Dialog)'    	'\1 bgcolor="darkcyan"\2'
%!postproc: '(<TD)(>RAC)'    		'\1 bgcolor="lime"\2'
%!postproc: '(<TD)(>Sed(Sokoban|Arkanoid))'	'\1 bgcolor="red"\2'

% Remove documentos
%!preproc: '^\| .* Coluna .*'		'%'
%!preproc: '^\| .* Dialog --tudo .*'	'%'
%!preproc: '^\| .* sed-HOWTO .*'	'%'

%!preproc: (NASCEU!)	**\1**

